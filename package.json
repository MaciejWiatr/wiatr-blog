{
	"name": "wiatr-blog",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"build:coverage": "cross-env INSTRUMENT_COVERAGE=true next build",
		"cypress": "cypress open",
		"cypress:headless": "cypress run",
		"e2e:build": "npm run build && npm run e2e",
		"e2e:dev": "start-server-and-test dev http://localhost:3000 cypress",
		"e2e": "start-server-and-test start http://localhost:3000 cypress",
		"e2e:headless": "start-server-and-test start http://localhost:3000 cypress:headless",
		"generate": "graphql-codegen -r dotenv/config",
		"prepare": "husky install",
		"e2e:prepush": "git-branch-is -q main && npm run e2e:build && npm run e2e:headless"
	},
	"dependencies": {
		"@apollo/client": "^3.5.6",
		"@graphql-codegen/cli": "^2.4.0",
		"@graphql-codegen/typescript": "^2.4.2",
		"@graphql-codegen/typescript-operations": "^2.2.2",
		"@graphql-codegen/typescript-react-apollo": "^3.2.4",
		"@graphql-codegen/typescript-react-query": "^3.3.2",
		"@next-auth/prisma-adapter": "^1.0.1",
		"@prisma/client": "^3.8.1",
		"@types/react-transition-group": "^4.4.4",
		"axios": "^0.24.0",
		"classnames": "^2.3.1",
		"framer-motion": "^5.5.6",
		"git-branch-is": "^4.0.0",
		"graphql": "^16.2.0",
		"nanoid": "^3.2.0",
		"next": "^12.0.9",
		"next-auth": "^4.1.2",
		"next-connect": "^0.11.0",
		"next-seo": "^4.29.0",
		"prisma": "^3.8.1",
		"react": "17.0.2",
		"react-cool-inview": "^2.0.8",
		"react-dom": "17.0.2",
		"react-icons": "^4.3.1",
		"react-loader-spinner": "^5.1.0",
		"react-markdown": "^7.1.2",
		"react-transition-group": "^4.4.2",
		"react-twemoji": "^0.3.0",
		"react-use": "^17.3.2",
		"rehype-raw": "^6.1.1",
		"remark-gfm": "^3.0.1",
		"sanitize-html": "^2.6.1",
		"sass": "^1.46.0",
		"swr": "^1.1.2",
		"validator": "^13.7.0",
		"zod": "^3.11.6",
		"zustand": "^3.6.8"
	},
	"devDependencies": {
		"@cypress/code-coverage": "^3.9.12",
		"@types/react": "^17.0.38",
		"@types/sanitize-html": "^2.6.2",
		"@types/validator": "^13.7.1",
		"autoprefixer": "^10.4.1",
		"babel-plugin-istanbul": "^6.1.1",
		"cross-env": "^7.0.3",
		"cypress": "^9.3.1",
		"eslint": "8.6.0",
		"eslint-config-next": "12.0.7",
		"husky": "^7.0.0",
		"nyc": "^15.1.0",
		"postcss": "^8.4.5",
		"postcss-flexbugs-fixes": "^5.0.2",
		"postcss-preset-env": "^7.2.0",
		"start-server-and-test": "^1.14.0",
		"tailwindcss": "^3.0.9",
		"typescript": "^4.5.4"
